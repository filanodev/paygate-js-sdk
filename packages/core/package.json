  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filanodev/paygate-js-sdk",
    "directory": "packages/core"
  },
  "funding": {
    "type": "individual",
    "url": "https://me.fedapay.com/filano_don"
