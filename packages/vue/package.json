{
  "name": "@filanodev/paygate-vue",
  "version": "0.1.1",
  "description": "PayGateGlobal Vue.js plugin with composables - Support FLOOZ and T-Money",
  "author": "Filano <info@filano.dev>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "paygate",
    "vue",
    "composables",
    "flooz",
    "tmoney",
    "payments",
    "togo",
    "africa"
  ],
  "dependencies": {
    "@filanodev/paygate-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.0.0 || ^3.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "vue": "^3.4.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filanodev/paygate-js-sdk",
    "directory": "packages/vue"
  },
  "funding": {
    "type": "individual",
    "url": "https://me.fedapay.com/filano_don"
  }
}