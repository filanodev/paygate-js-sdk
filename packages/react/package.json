{
  "name": "@filanodev/paygate-react",
  "version": "0.1.0",
  "description": "PayGateGlobal React hooks and components - Support FLOOZ and T-Money",
  "author": "Filano <info@filano.dev>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint src/**/*.{ts,tsx}",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "paygate",
    "react",
    "hooks",
    "flooz",
    "tmoney",
    "payments",
    "togo",
    "africa"
  ],
  "dependencies": {
    "@filanodev/paygate-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filano/paygate-js-sdk",
    "directory": "packages/react"
  }
}