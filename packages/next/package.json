{
  "name": "@filanodev/paygate-next",
  "version": "1.0.0",
  "description": "Plugin Next.js pour l'int√©gration de PayGateGlobal - Support FLOOZ et T-Money",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.cjs",
      "types": "./dist/client.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint src/**/*.{ts,tsx}",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@filanodev/paygate-core": "workspace:*"
  },
  "peerDependencies": {
    "next": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.64",
    "@vitejs/plugin-react": "^4.2.1",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filano/paygate-js-sdk.git",
    "directory": "packages/next"
  },
  "keywords": [
    "paygate",
    "paygateglobal",
    "payment",
    "flooz",
    "tmoney",
    "togo",
    "mobile-money",
    "nextjs",
    "react",
    "server-side-rendering"
  ],
  "author": {
    "name": "Filano",
    "url": "https://me.fedapay.com/filano_don"
  },
